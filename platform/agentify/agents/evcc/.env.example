# EVCC Agent Configuration

# Server Configuration
PORT=3003
NODE_ENV=development

# EVCC Configuration
EVCC_URL=http://localhost:7070
EVCC_API_TIMEOUT=30000

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# Infrastructure Agents (Optional)
MONITORING_AGENT_URL=http://localhost:3001
LOGGING_AGENT_URL=http://localhost:3002
REMOTE_ACCESS_AGENT_URL=http://localhost:3004

# Safety Limits
MAX_CURRENT_LIMIT_A=32
MIN_CURRENT_LIMIT_A=6
MAX_POWER_LIMIT_W=22080
MIN_POWER_LIMIT_W=1380

# Optimization Settings
DEFAULT_OPTIMIZATION_OBJECTIVE=maximize_pv
ENABLE_INFRASTRUCTURE_INTEGRATION=true

# Logging
LOG_LEVEL=info

