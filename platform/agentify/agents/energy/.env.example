# Energy Agent Configuration
PORT=3000
NODE_ENV=development

# Energy API Configuration
ENERGY_API_BASE_URL=http://localhost:8000/energy-api/v1

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Agent Configuration
AGENT_ID=agent.energy.controller
CUSTOMER_ID=customer-001

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/energy-agent.log

# Infrastructure Integration
ENABLE_INFRASTRUCTURE_INTEGRATION=true
MONITORING_AGENT_URL=http://localhost:3001
LOGGING_AGENT_URL=http://localhost:3002
REMOTE_ACCESS_AGENT_URL=http://localhost:3003

# Safety Limits
MAX_POWER_LIMIT_W=22080
MIN_POWER_LIMIT_W=1380
GRID_FREQUENCY_MIN_HZ=49.8
GRID_FREQUENCY_MAX_HZ=50.2

